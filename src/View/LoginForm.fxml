<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="509.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginForm">
   <children>
      <Pane layoutX="18.0" prefHeight="400.0" prefWidth="573.0">
         <children>
            <Button fx:id="loginButton" layoutX="366.0" layoutY="297.0" mnemonicParsing="false" onAction="#onActionLogin" prefHeight="35.0" prefWidth="139.0" text="Login" />
            <Label fx:id="userIdLabel" alignment="CENTER_RIGHT" layoutX="56.0" layoutY="102.0" prefHeight="83.0" prefWidth="240.0" text="User ID" textAlignment="CENTER">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Label fx:id="passwordLabel" alignment="CENTER_RIGHT" layoutX="56.0" layoutY="181.0" prefHeight="83.0" prefWidth="248.0" text="Password">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TextField fx:id="userIdField" layoutX="313.0" layoutY="114.0" prefHeight="54.0" prefWidth="240.0" promptText="Enter UserId">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <TextField fx:id="passwordField" layoutX="311.0" layoutY="190.0" prefHeight="54.0" prefWidth="240.0" promptText="Enter Password">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <Label fx:id="timeZoneLabel" alignment="CENTER_RIGHT" layoutX="10.0" layoutY="372.0" prefHeight="24.0" prefWidth="165.0" text="TimeZone:" textAlignment="JUSTIFY" />
            <Label fx:id="loginTimeZone" layoutX="180.0" layoutY="370.0" prefHeight="24.0" prefWidth="172.0" />
         </children></Pane>
      <Label fx:id="loginGreeting" layoutX="17.0" prefHeight="104.0" prefWidth="552.0" text="Welcome" textAlignment="RIGHT">
         <font>
            <Font size="36.0" />
         </font></Label>
   </children>
</AnchorPane>
