<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginForm">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Button fx:id="loginButton" layoutX="210.0" layoutY="303.0" mnemonicParsing="false" onAction="#onActionLogin" prefHeight="31.0" prefWidth="168.0" text="Login" />
            <Label fx:id="userIdLabel" layoutX="56.0" layoutY="102.0" prefHeight="83.0" prefWidth="240.0" text="User ID" textAlignment="CENTER">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Label fx:id="passwordLabel" layoutX="56.0" layoutY="181.0" prefHeight="83.0" prefWidth="248.0" text="Password">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TextField fx:id="userIdField" layoutX="263.0" layoutY="114.0" prefHeight="54.0" prefWidth="226.0" promptText="Enter UserId">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <TextField fx:id="passwordField" layoutX="263.0" layoutY="190.0" prefHeight="54.0" prefWidth="226.0" promptText="Enter Password">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <Label fx:id="timeZoneLabel" alignment="CENTER_RIGHT" layoutX="407.0" layoutY="14.0" prefHeight="17.0" prefWidth="111.0" text="TimeZone:" textAlignment="JUSTIFY" />
            <Label fx:id="loginTimeZone" layoutX="518.0" layoutY="14.0" prefHeight="17.0" prefWidth="76.0" />
         </children></Pane>
      <Label fx:id="loginGreeting" layoutX="53.0" prefHeight="83.0" prefWidth="326.0" text="Welcome" textAlignment="RIGHT">
         <font>
            <Font size="36.0" />
         </font></Label>
   </children>
</AnchorPane>
