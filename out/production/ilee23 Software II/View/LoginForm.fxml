<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginForm">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Button fx:id="loginButton" layoutX="257.0" layoutY="303.0" mnemonicParsing="false" onAction="#onActionLogin" prefHeight="31.0" prefWidth="121.0" text="Login" />
            <Label fx:id="userIdLabel" layoutX="96.0" layoutY="102.0" prefHeight="83.0" prefWidth="84.0" text="User ID" textAlignment="CENTER">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Label fx:id="passwordLabel" layoutX="96.0" layoutY="181.0" prefHeight="83.0" prefWidth="97.0" text="Password">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TextField fx:id="userIdField" layoutX="204.0" layoutY="114.0" prefHeight="54.0" prefWidth="227.0" promptText="Enter UserId">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <TextField fx:id="passwordField" layoutX="204.0" layoutY="190.0" prefHeight="54.0" prefWidth="227.0" promptText="Enter Password">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <Label fx:id="timeZoneLabel" layoutX="456.0" layoutY="14.0" prefHeight="17.0" prefWidth="62.0" text="TimeZone:" />
            <Label fx:id="loginTimeZone" layoutX="518.0" layoutY="14.0" prefHeight="17.0" prefWidth="76.0" />
         </children></Pane>
      <Label fx:id="loginGreeting" layoutX="236.0" prefHeight="83.0" prefWidth="193.0" text="Welcome" textAlignment="JUSTIFY">
         <font>
            <Font size="36.0" />
         </font></Label>
   </children>
</AnchorPane>
